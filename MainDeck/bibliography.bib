@article{huangAnalysisInhomogeneousYang2022,
  title = {The Analysis of Inhomogeneous {{Yang}}--{{Mills}} Connections on Closed {{Riemannian}} Manifold},
  author = {Huang, Teng},
  date = {2022-07-01},
  journaltitle = {Journal of Mathematical Physics},
  volume = {63},
  number = {7},
  pages = {071509},
  issn = {0022-2488, 1089-7658},
  doi = {10.1063/5.0088833},
  url = {https://pubs.aip.org/jmp/article/63/7/071509/2843552/The-analysis-of-inhomogeneous-Yang-Mills},
  urldate = {2025-01-04},
  abstract = {In this article, we study a class of connections on a closed Riemannian manifold X of dimensional n \&gt; 4, which we call inhomogeneous Yang--Mills connections. Some special cases included \textohm -Yang--Mills connections, where \textohm{} is a smooth (may be not closed) (n - 4)-form on X. We extend the known analytic results of pure Yang--Mills connections, which included the monotonicity formula and the {$\varepsilon$}-regularity theorem to the class of inhomogeneous Yang--Mills connections. Using those analytic results, we obtain the energy quantization and Uhlenbeck compactness for the moduli space of inhomogeneous Yang--Mills connections that have a uniformly Ln2-bounded curvature. A removable singularity theorem for singular inhomogeneous Yang--Mills connections on a bundle over the punctured ball is also proved. Finally, we also prove an energy gap result for inhomogeneous Yang--Mills connections under some mild conditions.},
  langid = {english}
}

