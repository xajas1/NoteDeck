@article{feehanLojasiewiczSimonGradientInequalities2020,
  title = {Lojasiewicz-{{Simon}} Gradient Inequalities for Analytic and {{Morse-Bott}} Functions on {{Banach}} Spaces},
  author = {Feehan, Paul M. N. and Maridakis, Manousos},
  date = {2020-08-01},
  journaltitle = {Journal f\"ur die reine und angewandte Mathematik (Crelles Journal)},
